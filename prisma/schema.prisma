// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

model Evento {
  id          Int      @id @default(autoincrement())
  data        DateTime
  local       String
  name        String
  description String
}

model Campanha {
  id          Int    @id @default(autoincrement())
  title       String
  description String
}

model Tutoriais {
  id    Int    @id @default(autoincrement())
  name  String
  video String
}
